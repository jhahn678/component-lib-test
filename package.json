{
  "name": "@jhahn678/component-lib-test",
  "version": "1.0.6",
  "main": "build/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test-build": "ts-node scripts/build --project web",
    "build": "npm run build-web && npm run build-mobile",
    "modify-web-imports": "ts-node scripts/modify-imports",
    "build-mobile": "rm -rf build/mobile && tsc --outDir build/mobile --declarationDir build/mobile/lib",
